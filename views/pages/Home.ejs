    <script>

    function newCookie(cookieName, cookieValue, expireMinutes) {
        var expires;

        if (expireMinutes) {
            var date = new Date();
            date.setTime(date.getTime() + (expireMinutes * 60 * 1000));
            expires = "; expires=" + date.toGMTString();
        }
        else {
            expires = "";
        }

        document.cookie = encodeURIComponent(cookieName) + "=" + encodeURIComponent(cookieValue) + expires + "; path=/";
    }

updateContent = function (id) {
    $.ajax({
        url: '/Home/UpdateCustomerClass',
        type: 'post',
        data: { customerClassId: id },
        success: function (response) {
            if (response.Sucess) {
                UpdateTestimonialHomeList();
                UpdateHomePageSpecials();
                UpdateHomePagePopularProducts();
                UpdateProductMenu();
                UpdateHomePageCategories();
                newCookie('keepCustomerClassCookie', JSON.stringify(id), 60);
                location.reload();
            }
        }
    });
};



